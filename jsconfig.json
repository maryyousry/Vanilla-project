{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@constants/*": ["./src/constants/*"],
      "@forms/*": ["./src/forms/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@pages/*": ["./src/pages/*"],
      "@routes/*": ["./src/routers/*"],
      "@services/*": ["./src/services/*"],
      "@store/*": ["./src/store/*"],
      "@schemas/*": ["./src/schemas/*"],
      "@UI/*" : ["./src/UI/*"],
      "@utils/*": ["./src/utils/*"],
    }
  },
  "exclude": ["node_modules"]
}